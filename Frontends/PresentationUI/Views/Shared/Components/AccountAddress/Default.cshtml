@model List<ResultAddressDto>

<div class="tab-pane" id="address">
    <h4 class="billing-address">Sipariş ve Fatura Adresi</h4>
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-xl-6">
                <a href="#" class="view">Düzenle</a>
                <p><strong>@item.AddressTitle</strong></p>
                <address>
                    <span><strong>Adı-Soyadı:</strong> @item.Name @item.Surname</span>
                    <br />
                    <span><strong>Adres:</strong> @item.AddressDetail1</span>,
                    <br />
                    <span>@item.AddressDetail2</span>,
                    <br />
                    <span>@item.District / @item.City</span>
                    <br />
                    <span><strong>Ülke:</strong> @item.Country</span>
                    <br />
                    <span><strong>Telefon:</strong> @item.Phone</span>
                    <br />
                    <span><strong>E-Posta:</strong> @item.Email</span>
                </address>
            </div>
        }
    </div>
</div>